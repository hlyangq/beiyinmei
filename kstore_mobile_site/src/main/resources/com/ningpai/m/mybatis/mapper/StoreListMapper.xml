        (third_id)
        FROM
        np_third_grantcat
        WHERE
        cate_id IN (SELECT DISTINCT
        (cat_id)
        FROM
        np_goods_category
        WHERE
        cat_parent_id IN (SELECT DISTINCT
        (cat_id)
        FROM
        np_goods_category
        WHERE
        cat_parent_id IN (SELECT
        (cat_id)
        FROM
        np_goods_category
        WHERE
